{
  "name": "athletics",
  "version": "1.0.0",
  "private": true,
  "description": "Gulp project",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "7.1.x",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "2.18.x",
    "gulp": "^3.9.1",
    "gulp-csso": "^3.0.0",
    "gulp-imagemin": "^3.4.0",
    "gulp-less": "3.3.x",
    "gulp-plumber": "1.1.x",
    "gulp-postcss": "7.0.x",
    "gulp-posthtml": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-svgstore": "^6.1.0",
    "gulp-webp": "^2.3.0",
    "run-sequence": "^2.2.0",
    "stylelint": "8.1.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint 'less/**/*.less' --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build && babel js -d build/js --presets es2015",
    "watch": "babel js -d build/js --presets es2015 -w",
    "start": "npm run build && gulp serve"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.html",
    "*.json",
    "*.js",
    "js/**/*.js",
    "img/**/*.svg",
    "less/**/*.less"
  ],
  "engines": {
    "node": "8.2"
  },
  "dependencies": {
    "del": "^3.0.0",
    "gulp-svgmin": "^1.2.4",
    "normalize.css": "^7.0.0",
    "npm": "^5.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeIv/almatyathletics.git"
  },
  "keywords": [
    "athletics"
  ],
  "author": "Mike Ivanov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MikeIv/almatyathletics/issues"
  },
  "homepage": "https://github.com/MikeIv/almatyathletics#readme"
}
